#!/bin/bash

#SBATCH --array=0-16
#SBATCH --cpus-per-task=16
#SBATCH --mem=249G
#SBATCH --time=30:00:00
#SBATCH -p epyc-256

module load anaconda3/latest

source activate ~/.conda/envs/hiwi-polaron

cd ~/Antiprotonic-atoms/

for ((z=0;z<=15;z++))
do
        python v5_HPC_ccs_run.py /scratch/vym17xaj/HPC_dynamics $(bc <<< "(($SLURM_ARRAY_TASK_ID)*16)+$z") &
done
wait
